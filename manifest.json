{
	"browser_action": {
		"browser_style": true,
		"default_popup": "tabs.html",
		"default_title": "Session tabs"
	},
	"sidebar_action": {
		"browser_style": true,
		"default_title": "Session tabs",
		"default_panel": "sidebar.html",
		"default_icon": {
			"19": "icons/window19.png",
			"38": "icons/window38.png"
		}
	},
	"icons": {
		"48": "icons/window.png",
		"96": "icons/window@2x.png"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": []
		}
	],
	"background": {
		"scripts": []
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "sessiontabs@eltharin.fr"
		}
	},
	"description": "Ajoute une bordure rouge pleine sur l'ensemble des pages web mozilla.org.",
	"icons": {
		"48": "icons/border-48.png"
	},
	"manifest_version": 2,
	"name": "Session tabs",
	"permissions": [
		"tabs", "menus", "theme",
		"bookmarks", "webRequest", "<all_urls>", "storage"
	],
	"version": "0.1"
}